{
  "name": "formkeeper",
  "version": "0.4.0",
  "private": true,
  "repository": {
    "url": "https://github.com/formkeeper/extension.git",
    "type": "git"
  },
  "dependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.3",
    "babel-eslint": "10.x",
    "babel-jest": "^26.1.0",
    "babel-preset-react-app": "^9.0.2",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "6.x",
    "eslint-config-react-app": "^5.0.1",
    "eslint-loader": "2.2.1",
    "eslint-plugin-flowtype": "3.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "file-loader": "4.2.0",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "jest": "^26.1.0",
    "npm-watch": "^0.6.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prop-types": "^15.7.2",
    "raf": "3.4.0",
    "react": "^16.13.1",
    "react-dev-utils": "^9.0.0-next.68",
    "react-dom": "^16.13.1",
    "react-frame-component": "^4.0.0",
    "react-helmet": "^6.1.0",
    "react-window": "^1.8.5",
    "resolve": "1.6.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "0.6.2",
    "webpack-dev-server": ">=3.1.11",
    "webpack-manifest-plugin": "2.0.4",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build-prod.js",
    "test": "node scripts/test.js",
    "test:unit": "node scripts/test.js --unit",
    "test:cy": "node scripts/test.js --e2e",
    "build:dev": "node scripts/build-dev.js",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "src",
        "public"
      ],
      "extensions": "js,html,css,json,svg"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@peculiar/webcrypto": "^1.1.2",
    "@types/chrome": "^0.0.121",
    "@types/jest": "^26.0.5",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "babel-loader": "^8.0.4",
    "browser-sync": "^2.26.7",
    "cypress": "4.10.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-react-hooks": "^2.0.1",
    "jest-environment-jsdom": "^26.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "react-devtools": "^4.7.0",
    "typescript": "^3.9.7",
    "wait-on": "^5.1.0",
    "webpack": "4.16.5",
    "webpack-cli": "3.1.0"
  }
}
