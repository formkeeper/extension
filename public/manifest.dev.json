{
  "short_name": "formkeeper",
  "name": "Formkeeper",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "app/background.js",
      "app/UNSAFE_cy-hook.js"
    ]
  },
  "browser_action": {},
  "permissions": [
    "tabs",
    "storage",
    "unlimitedStorage"
  ],
  "content_scripts" : [
    {
      "matches": [ "<all_urls>" ],
      "css": ["/css/root.css"],
      "js": [
        "/static/js/content.js",
        "/static/js/UNSAFE_cy-inject.js"
      ],
      "all_frames": true
    }
  ],
  "web_accessible_resources":[
    "/static/css/content.css", "/static/media/*"
  ],
  "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'"
}
